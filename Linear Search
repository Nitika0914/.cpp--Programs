#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cout<<"enter array: ";
        cin>>arr[i];
    }
    int num,flag=1;
    cout<<"enter num: ";
    cin>>num;
    for(int i=0;i<n;i++){
        if(arr[i]==num){
            cout<<"present at index - "<<i;
            flag=0;
        } 
    }
    if(flag==1){
        cout<<"-1";
    }
    return 0;
}
